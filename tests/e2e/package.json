{
    "name": "cypress-sapling",
    "version": "2.0.0",
    "description": "A skeleton for every Cypress repository",
    "main": "",
    "scripts": {
        "test-single": "yarn run lint-fix && cypress run --spec process.env.spec"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{js,json,md}": [
            "prettier --write",
            "git add"
        ],
        "**/*.js": [
            "eslint",
            "git add"
        ]
    },
    "eslintIgnore": [
        "artifacts/**/*.js"
    ],
    "eslintConfig": {
        "root": true
    },
    "author": "Diogo Nunes",
    "license": "MIT",
    "devDependencies": {
        "@testing-library/cypress": "^4.2.0",
        "cypress": "^4.11.0",
        "cypress-commands": "^0.2.0",
        "cypress-dark": "^1.7.14",
        "cypress-multi-reporters": "^1.4.0",
        "cypress-plugin-retries": "^1.5.2",
        "cypress-select-tests": "^1.5.7",
        "cypress-wait-until": "^1.7.1",
        "eslint": "^6.8.0",
        "eslint-plugin-cypress": "^2.11.1",
        "git-cz": "^3.3.0",
        "husky": "^3.1.0",
        "lint-staged": "^9.5.0",
        "mocha": "5.2.0",
        "mocha-junit-reporter": "^1.23.3",
        "mochawesome": "^4.1.0",
        "mochawesome-merge": "^2.1.0",
        "mochawesome-report-generator": "^4.1.0",
        "prettier": "^1.19.1"
    },
    "dependencies": {
        "syncyarnlock": "^1.0.19"
    },
    "private": true
}
